<view class="page">
    <view class="page__hd">
        <view class="page__title">{{name}}</view>
        <view class="page__desc">{{username}}</view>
    </view>
    <view class="page__bd">
        <view class="weui-form-preview">
            
            <view class="weui-form-preview__hd">
                <view class="weui-form-preview__label">最新密码</view>
                <view class="weui-form-preview__value_in-hd codeStyle">{{code}}</view>
            </view>
            <progress percent="{{timer}}" stroke-width="4" />
            <view class="weui-form-preview__bd">
                
                <view class="weui-form-preview__item">
                    <view class="weui-form-preview__value">　　{{desc}}</view>
                </view>
            </view>
            <view class="weui-form-preview__ft">
               
                <view class="weui-form-preview__btn weui-form-preview__btn_default" hover-class="weui-form-preview__btn_active" catchtap="deleteOne">删除</view>
                <navigator class="weui-form-preview__btn weui-form-preview__btn_primary" hover-class="weui-form-preview__btn_active" url="../qrcode/qrcode?id={{keys}}&username={{username}}&name={{name}}&desc={{desc}}">生成二维码</navigator>
            </view>
        </view>
    </view>
</view>
<image src="data:image/png;base64," catchtap="navigateBack" class="footBtn"></image>